{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": ["error", "never"],
        "max-len": ["error", { "code": 120 }],
        "react/prop-types": "off",
        "require-jsdoc": "off",
        "no-unused-vars": "warn"
    }
}
